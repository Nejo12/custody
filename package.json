{
  "name": "custody",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest run",
    "lh:prepare": "mkdir -p .lighthouse || true",
    "lh:desktop": "npm run lh:prepare && lighthouse http://localhost:3000 --preset=desktop --output=json --output-path ./.lighthouse/desktop.json --quiet --chrome-flags=\"--headless=new\"",
    "lh:mobile": "npm run lh:prepare && lighthouse http://localhost:3000 --preset=mobile --output=json --output-path ./.lighthouse/mobile.json --quiet --chrome-flags=\"--headless=new\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.80.0",
    "@upstash/redis": "^1.35.6",
    "classnames": "^2.5.1",
    "framer-motion": "^12.23.24",
    "jszip": "^3.10.1",
    "next": "16.0.1",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tesseract.js": "^5.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jszip": "^3.4.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.6",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.0.1",
    "lighthouse": "^13.0.1",
    "pdfjs-dist": "^5.4.394",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.6"
  }
}
